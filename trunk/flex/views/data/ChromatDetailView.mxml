      <mx:Canvas label="Quality" width="100%" height="100%" minHeight="0">
        <mx:VBox width="100%">
          
          <!--MAYBE ADD A TRIMMED REGION BAR-->
          <mx:LineChart id="qualityGraph" 
                        gutterBottom="40" 
                        gutterTop="20" 
                        gutterLeft="65" 
