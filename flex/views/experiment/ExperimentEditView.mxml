					sampleSetupView.sampleOrganismCombo.selectedIndex = -1;
					sampleSetupView.enabled = false;
					sampleSetupView.otherSamplePrepMethod.text = "";
					sampleSetupView.seqPrepRadioGroup.selectedValue = 'Y';
					sampleSetupView.prePooledSamplesRadioGroup.selectedValue = 'N';
					
					if(sampleSetupView.prePooledNumTubes != null){
