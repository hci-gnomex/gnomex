			private function filterQCAmendRequestCategory(item:Object):Boolean {
				//parentApplication.dictionaryManager.xml.Dictionary.(@className == 'hci.gnomex.model.RequestCategory').DictionaryEntry.(@isActive == 'Y' &amp;&amp; @value != '' &amp;&amp; @isInternal == 'Y' &amp;&amp; ( @type == parentApplication.TYPE_MICROARRAY ||  @isIlluminaType == 'Y')) 
				var keep:Boolean = filterRequestCategoryByCore(item);
				if (keep && (parentApplication.isAdminState || parentApplication.isSuperAdmin) && parentApplication.coreFacilitiesICanManage.length() >= 1) {
					keep = filterRequestCategoryByCoreForAdmins(item);
				} 
				if (item.@type != parentApplication.TYPE_MICROARRAY && item.@isIlluminaType != 'Y') {
					keep = false;
				}
				return keep;
			}
			
			private function filterAddLanesAmendRequestCategory(item:Object):Boolean {
				//parentApplication.dictionaryManager.xml.Dictionary.(@className == 'hci.gnomex.model.RequestCategory').DictionaryEntry.(@isActive == 'Y' &amp;&amp; @value != '' &amp;&amp; @isInternal == 'Y' &amp;&amp; @isIlluminaType == 'Y')				var keep:Boolean = false;
				var keep:Boolean = filterRequestCategoryByCore(item);
				if (keep && (parentApplication.isAdminState || parentApplication.isSuperAdmin) && parentApplication.coreFacilitiesICanManage.length() >= 1) {
					keep = filterRequestCategoryByCoreForAdmins(item);
				}
				if (item.@isIlluminaType != 'Y') {
					keep = false;
				}
				return keep;
			}
			
			private function filterRequestCategoryByCore(item:Object):Boolean {
				var keep:Boolean = false;
				if (item.@isActive == 'Y' && item.@value != '' && item.@isInternal == 'Y') {
					if (item.@isClinicalResearch == null || item.@isClinicalResearch != 'Y') {
						if (item.@idCoreFacility == null) {
							// If a request category is not associated with a core facility,
							// include it in the list.
							keep = true;
						} else if (lab != null) {
							// filter by the lab's core facility
							for each(var cf:Object in lab..CoreFacility) {
								if (cf.@idCoreFacility == item.@idCoreFacility) {
									if ( parentApplication.getCoreFacilityProperty(item.@idCoreFacility, parentApplication.PROPERTY_ALLOW_ADD_SEQUENCING_SERVICES) == null || 
										parentApplication.getCoreFacilityProperty(item.@idCoreFacility, parentApplication.PROPERTY_ALLOW_ADD_SEQUENCING_SERVICES) != 'N' ) {
										keep = true;
									}
									break;
								}
							}
						}
					}
				}
				return keep;
				
			}
			
			private function filterRequestCategoryByCoreForAdmins(item:Object):Boolean {
				var keep:Boolean = false;
				for each (var cf:Object in parentApplication.coreFacilitiesICanManage) {
					if (cf.@idCoreFacility == item.@idCoreFacility) {
						keep = true;
						break;
					}
				}
				return keep;
			}
			
			private function getTitle(currentState:String, requestCategoryName:String, coreFacility:Object):String {
				var title:String  = "";
				var catName:String = requestCategoryName != '' && requestCategoryName != null ? " " + requestCategoryName : '';
				var coreName:String = coreFacility != null ? ' For ' + coreFacility.@facilityName : '';
				if (parentApplication.isInternalExperimentSubmission) {
					if (isEditState()) {
						title = "Edit" + catName + " Experiment";
					} else if (isAmendState()) {
						title = getAmendTitleText();
					} else {
						title = "New" + catName + " Experiment";
						title += coreName;
					}
				} else {
					if (isEditState()) {
						title = "Edit External " + catName + " Experiment";
					} else {
						title = "Upload" + catName + " experiment data from a third party facility";
						title += coreName;
					}
				}
				return title;
			}
			
			private function determineIfProductsNeeded():Boolean {
				if (parentApplication.submitInternalExperiment() && requestCategory != null) {
					if (requestCategory.@idProductType != null && requestCategory.@idProductType != '') {
						return true;
					}
				}
				
				return false;
			}
			
			private function updateDirtyNote(isDirty:Boolean):void {
				if (isDirty && !this.isDirty) {
					this.isDirty = true;
					this.dirty.setDirty();
				} else if (!isDirty && this.isDirty) {
					this.isDirty = false;
					this.dirty.resetDirty();
				}
			}
			
			private function handleDirtyEvent(event:DirtyEvent):void {
				if (event.type == DirtyEvent.NOW_DIRTY) {
					updateDirtyNote(true);
				} else if (event.type == DirtyEvent.NOW_CLEAN) {
					updateDirtyNote(false);
				}
			}
			
			private function showProductsTab(allowEditing:Boolean):void {
				if (theTab.contains(productsView) && productsView != null) {
					productsView.allowEdits(allowEditing);
