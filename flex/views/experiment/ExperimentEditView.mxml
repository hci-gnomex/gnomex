				if (this.radioGroupAmendType.selectedValue != null && this.radioGroupAmendType.selectedValue != "") {
					if (this.radioGroupAmendType.selectedValue.toString().indexOf("ADD-LANES-") == 0) {
						this.currentState = "SolexaLaneAmendState";
						setAmendTitle();
