			private function refreshLab():void {
				if (requestEditLabCombo.selectedItem != null && requestEditLabCombo.selectedItem.@idLab != '') {
					
					var params:Object = new Object();
					params.idLab = requestEditLabCombo.selectedItem.@idLab;
