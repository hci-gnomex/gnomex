					productsView.reset();
					productsView.allowEdits(false);
					productsView.enabled = false;
					theTab.getTabAt( theTab.getChildIndex( productsView ) ).visible = false;
					theTab.getTabAt( theTab.getChildIndex( productsView ) ).includeInLayout = false;
				}
			}
			
