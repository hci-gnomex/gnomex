			private function onGetLab(event:ResultEvent):void {
				if (getLab.lastResult.name() == "OpenLabList") {
					var lab:Object = getLab.lastResult..Lab.(@idLab == requestEditLabCombo.selectedItem.@idLab);
					
					billingAccounts = new XMLListCollection(lab.billingAccounts.BillingAccount);
					filteredBillingAccounts = new XMLListCollection(lab.approvedBillingAccounts.BillingAccount);
					var emptyBillingAccount:XML = <BillingAccount idBillingAccount='' accountName='' />
					filteredBillingAccounts.addItemAt(emptyBillingAccount, 0);
					filteredBillingAccounts.filterFunction = filterBillingAccounts;
					filteredBillingAccounts.refresh();
					var found:Boolean = false;
					for each(var ba:Object in filteredBillingAccounts) {
						if (ba.@idBillingAccount == request.@idBillingAccount) {
							found = true;
							requestEditBillingAccountCombo.selectedItem = ba;
							break;
						}
					}
					if (!found) {
						requestEditBillingAccountCombo.selectedItem = filteredBillingAccounts[0];
					}
					
					requestEditProjectCombo.selectedItem = null;
					projects = new XMLListCollection(getLab.lastResult..Lab.projects.Project);
					for each(var p:Object in projects) {
						if (p.@idProject == request.@idProject) {
							requestEditProjectCombo.selectedItem = p;
							break;
						}
					}
					if (requestEditProjectCombo.selectedItem == null && request.hasOwnProperty("@idProject") && request.@idProject != null && request.@idProject != '') {
						var proj:XML = new XML("<Project name='" + request.@projectName + "' display='" + request.@projectName + "' idProject = '" + request.@idProject + "' />");
						projects.addItem(proj);
						requestEditProjectCombo.selectedIndex = 1;
						requestEditProjectCombo.enabled = false;
					} else {
						requestEditProjectCombo.enabled = true;
					}
					
					if (this.isSolexaEditState()) {
						this.requestApplicationComboBox.selectedItem = null;
						for each(var p1:Object in filteredAppList) {
							if (p1.@value == request.@codeApplication) {
								requestApplicationComboBox.selectedItem = p1;
								break;
							}
						}
					}
					
					if (request.hasOwnProperty("@isExternal") && request.@isExternal == "Y") {
						for each(var organism:Object in parentApplication.dictionaryManager.xml.Dictionary.(@className == 'hci.gnomex.model.OrganismLite').DictionaryEntry) {
							if (organism.@value == request.@idOrganism) {
								this.sampleOrganismCombo.selectedItem = organism;
								break;
							}
						}
					}
					
					// set up Owner Combo
					possibleRequestOwners = createAppUsersForLab(lab);
					addCurrentRequestOwnerToList();
					appUserSort = new Sort();
					appUserSort.compareFunction = this.sortAppUsers;
					possibleRequestOwners.sort = appUserSort;
					possibleRequestOwners.refresh();
					ownerCombo.selectedItem = possibleRequestOwners.getItemAt(0);
					for each(var a1:Object in possibleRequestOwners) {
						if (a1.@idAppUser == request.@idAppUser) {
							ownerCombo.selectedItem = a1;
							break;
						}
					}
					ownerCombo.enabled = true;
					
					// set up Submitter Combo
					possibleRequestSubmitters = createSubmittersForLab(lab);
					addCurrentRequestSubmitterToList();
					possibleRequestSubmitters.sort = appUserSort;
					possibleRequestSubmitters.refresh();
					submitterCombo.selectedItem = possibleRequestSubmitters.getItemAt(0);
					for each(var a2:Object in possibleRequestSubmitters) {
						if (a2.@idAppUser == request.@idSubmitter) {
							submitterCombo.selectedItem = a2;
							break;
						}
					}
					//submitterCombo.enabled = true;
				}			
				
			}
			
			private function createAppUsersForLab(lab:Object):XMLListCollection {
				var emptyNode:XML = <AppUser idAppUser='' displayName=''/>;
				var list:XMLListCollection = new XMLListCollection(lab.members.AppUser);
				for each (var mgr:Object in lab.managers.AppUser) {
					var mgrInLabList:Boolean = false;
					for each(var a:Object in lab.members.AppUser) {
						if (a.@idAppUser == mgr.@idAppUser) {
							mgrInLabList = true;
							break;
						}
					}
					if (!mgrInLabList) {
						list.addItem(mgr);				
					}
				}
				list.addItemAt(emptyNode, 0);
				return list;
			}
			
			private function createSubmittersForLab(lab:Object):XMLListCollection {
				var list:XMLListCollection = new XMLListCollection(createAppUsersForLab(lab).copy());
				for each (var submitter:Object in request.submitterFromOtherCores.AppUser) {
					var submitterInLabList:Boolean = false;
					for each(var a:Object in list) {
						if (a.@idAppUser == submitter.@idAppUser) {
							submitterInLabList = true;
							break;
						}
					}
					if (!submitterInLabList) {
						list.addItem(submitter);				
					}
				}
				return list;
			}
			
			private function addCurrentRequestOwnerToList():void {
				var isNecessary:Boolean = true;
				for each(var owner:Object in possibleRequestOwners) {
					if (owner.@idAppUser == request.@idAppUser) {
						isNecessary = false;
						break;
					}
				}
				if (isNecessary) {
					var currentRequestOwner:Object = parentApplication.dictionaryManager.getEntry('hci.gnomex.model.AppUserLite', request.@idAppUser);
					var node:XML = new XML("<AppUser " +
						"idAppUser='" + GNomExStringUtil.makeXMLSafe(currentRequestOwner.@idAppUser) + "' " +
						"displayName='" + GNomExStringUtil.makeXMLSafe(currentRequestOwner.@display) + "' />");
					possibleRequestOwners.addItem(node);
				}
			}
			
			private function addCurrentRequestSubmitterToList():void {
				var isNecessary:Boolean = true;
				for each(var submitter:Object in possibleRequestSubmitters) {
					if (submitter.@idAppUser == request.@idSubmitter) {
						isNecessary = false;
						break;
					}
				}
				if (isNecessary) {
					var currentRequestSubmitter:Object = parentApplication.dictionaryManager.getEntry('hci.gnomex.model.AppUserLite', request.@idSubmitter);
					var node:XML = new XML("<AppUser " +
						"idAppUser='" + GNomExStringUtil.makeXMLSafe(currentRequestSubmitter.@idAppUser) + "' " +
						"displayName='" + GNomExStringUtil.makeXMLSafe(currentRequestSubmitter.@display) + "' />");
					possibleRequestSubmitters.addItem(node);
				}	
			}	
			
