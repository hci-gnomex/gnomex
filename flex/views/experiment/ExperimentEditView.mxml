			private function showConversionWindow():void{
				var convertWindow:ConvertIlluminaWindow = new ConvertIlluminaWindow();
				convertWindow.request = this.request;
				
				if(request.@codeRequestCategory == parentApplication.TYPE_HISEQ){
					convertWindow.convertToCategory = parentApplication.TYPE_MISEQ;
				} else if(request.@codeRequestCategory == parentApplication.TYPE_MISEQ){
					convertWindow.convertToCategory = parentApplication.TYPE_HISEQ;
				}
				
				PopUpManager.addPopUp(convertWindow,this, true)
				this.setFocus();
				//PopUpManager.centerPopUp(convertWindow);
				var x:Number = flash.system.Capabilities.screenResolutionX;
				var y:Number = flash.system.Capabilities.screenResolutionY;
				callLater(function():void{
					convertWindow.move((x/2)-500, (y/2) - 500);
				});
				convertWindow.init();
			}
			
			public function refreshAmendSetup():void {
				theTab.selectedChild = amendSetupView;
				
				if (labCombo != null) {
					labCombo.selectedIndex = -1;
					labCombo.selectedItem = null;
					lab = null;
				}
				
				if (appUserCombo != null) {
					appUserCombo.selectedIndex = -1;
					appUserCombo.selectedItem = null;
				}
				
				if (radioGroupAmendType != null && radioGroupAmendType.selection != null) {
					radioGroupAmendType.selectedValue = null;
					radioGroupAmendType.selection.selected = false;
					radioGroupAmendType.selection = null;
