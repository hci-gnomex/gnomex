			private function changeIdBillingAccount(event:CloseEvent):void {
				var proceed:Boolean = false;
				if (event.detail == Alert.NO && !warnToSelectNewBillingAccount) {
					request.@idBillingAccount = requestEditBillingAccountCombo.selectedItem.@idBillingAccount;
					proceed = true;
				} else if (event.detail == Alert.NO && warnToSelectNewBillingAccount) {
					warnToSelectNewBillingAccount = false;
					Alert.show("Please select a new billing account.", "Notice", Alert.OK, this);
				} else if (event.detail == Alert.YES) {
					proceed = true;
				}
				if (proceed) {
					billingAccountAlreadyUpdated = true;
					saveRequest();
				}
			}
			
