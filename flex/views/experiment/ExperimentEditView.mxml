			public function getSampleOrganisms():XMLListCollection {
				var sampleOrgs:XMLListCollection = new XMLListCollection();
				var organismDict:Object = new Object();
				
				
				var samples:XMLListCollection = new XMLListCollection(this.samples.copy());
				//If we are in new experiment submit state we need to grab the organisms off of the samples
				
				for each(var seq:XML in this.lanes){
					for each(var smp:XML in samples){
						if(seq.@idSample == smp.@idSample){
							seq.@idOrganism = smp.@idOrganism;
							seq.@organism = parentApplication.dictionaryManager.getEntry('hci.gnomex.model.OrganismLite', smp.@idOrganism).@display;
						}
					}
				}
				for each(var seq1:XML in this.lanes){
					organismDict[seq1.@idOrganism] = seq1;
				}
				
				for each(var value:XML in organismDict){
					sampleOrgs.addItem(value);
				}
				
				
				return sampleOrgs;
				
			}
			
